@model Targetcom.Models.ViewModels.ViewProfileVM;
<style>
    .gap {
        gap: 1rem;
    }

    .card-footer .d-flex > a:hover {
        text-decoration: none;
    }

    .viewallbtn {
        width: 100%;
        font-family: Poppins, sans-serif;
    }

        .viewallbtn:hover {
            background-color: #eeeeee;
            text-decoration: none;
        }
        .night .viewallbtn:hover {
            background-color: #1f1f1f;
            text-decoration: none;
        }

    .disable {
        display: none;
    }

    .btnedit {
        width: 100%;
        font-family: Poppins, sans-serif;
    }

        .btnedit:hover {
            background-color: #eeeeee;
            text-decoration: none;
        }

    .middletabbtn:hover {
        text-decoration: none;
    }

    .middletabbtn1 {
        background-color: transparent;
        text-decoration: none;
    }

        .middletabbtn1:hover {
            background-color: #eeeeee;
            text-decoration: none;
        }

    #summernote {
        font-size: 14px;
        outline: 0 !important;
        border: 0;
    }

    .dropbtn {
        color: white;
        border: none;
        cursor: pointer;
        background-color: transparent;
    }

    /* The container <div> - needed to position the dropdown content */
    .dropdown {
        position: relative;
        display: inline-block;
        transition: .2s;
    }

    /* Dropdown Content (Hidden by Default) */
    .dropdown-content {
        display: none;
        position: absolute;
        min-width: 160px;
        z-index: 1;
        right: 0;
        transition: .2s;
    }

        /* Links inside the dropdown */
        .dropdown-content button {
            display: block;
        }

    /* Show the dropdown menu on hover */
    .dropdown:hover .dropdown-content {
        display: block;
    }

    .btnlike:hover {
        text-decoration: none;
    }

    img {
        object-fit: cover;
    }
    .objf {
        object-fit: cover;
        background-size: 116px 120px;
    }

    .objfeffect {
        opacity: 0%;
        transition: .1s;
    }

        .objfeffect:hover {
            opacity: 100%;
            background-color: rgba(0, 0, 0, 0.60);
        }
    .rainbow {
        animation: textAnimate 12s linear infinite alternate;
    }

    @@keyframes textAnimate {

        0% {
            color: #98ff00;
        }

        25% {
            color: #6000ff;
        }

        50% {
            color: #ff6b00;
        }

        75% {
            color: #007aff;
        }

        100% {
            color: #ff0000;
        }
    }
    .grid {
        top: -6.5%;
        left: -6.5%;
        background-size: 106.5% 106.5%;
        object-fit: cover;
        width: 106.5%;
        height: 106.5%;
    }
</style>
<div class="gap d-flex gap day_color_font" style="font-family: Poppins, sans-serif;">
    <partial name="SideBar" model="new Tuple<int, int>(Model.IdentityProfile.Friendships.Count(w => w.FriendStatus == Env.Invite && w.FriendId == Model.IdentityProfile.Id),0)" />

    @if (Model.FindedProfile.Banned is not null)
    {
        <div style="width: 100%;" class="gap d-flex flex-column">
            <div class="alert day_back_head shadow-lg p-5 d-flex justify-content-center align-items-center flex-column" style="font-family: Poppins, sans-serif;">
                <div>
                    <h4 class="text-center">@Model.FindedProfile.Name @Model.FindedProfile.Surname has banned</h4>
                    <hr />
                    <h5>
                        Admin / moderator: @if (Model.FindedProfile.Banned.AdminId != Model.IdentityProfile.Id)
                        {
                            <a class="text-primary" style="cursor:pointer;" asp-controller="Friends" asp-action="ViewProfile" asp-route-Id="@Model.FindedProfile.Banned.AdminId">@Model.FindedProfile.Banned.Admin.Name @Model.FindedProfile.Banned.Admin.Surname</a>
                        }
                        else
                        {
                            <a class="text-primary" style="cursor:pointer;" asp-controller="Profile" asp-action="Index">@Model.FindedProfile.Banned.Admin.Name @Model.FindedProfile.Banned.Admin.Surname</a>
                        }
                    </h5>
                    <hr />
                    <h5>Reason: <a class="text-primary" style="cursor:pointer;">@Model.FindedProfile.Banned.Reason</a></h5>
                    <hr />
                    <h5>Banned date/time: <a class="text-primary" style="cursor:pointer;">@Model.FindedProfile.Banned.TimeStamp.ToString("dd/MM/yyyy - HH:mm") ua</a></h5>
                    @if (Model.FindedProfile.Banned.IsPermanent)
                    {
                        <hr />
                        <h5>This profile always non-active. Profile on sets permanent banned</h5>
                    }
                    else
                    {
                        <hr />
                        @if (DateTime.Now > Model.FindedProfile.Banned.ReasonDate)
                        {
                            <h5 class="text-success">Unblocked after authorization account!</h5>
                        }
                        else
                        {
                            <h5>Reason end date/time: <a class="text-primary" style="cursor:pointer;">@Model.FindedProfile.Banned.ReasonDate.ToString("dd/MM/yyyy - HH:mm") ua</a></h5>
                        }
                    }
                </div>
            </div>
        </div>
    }
    else
    {
        <div class="d-flex gap" style="width: 100%;">
            <div style="width: 15rem;" class="gap d-flex flex-column">
                <div class="day_back_head p-4 d-flex flex-column gap overflow-hidden rounded">
                    <div class="align-self-center">
                        @if (Model.FindedProfile.AvatarGridURL != null || Model.FindedProfile.AvatarGridURL != string.Empty)
                        {
                            @if (Model.FindedProfile.UrlAvatar == Targetcom.Env.DefaultImageUrl)
                            {
                                <div class="position-relative overflow-hidden p-1 d-flex justify-content-center align-items-center rounded">
                                    <img src="@Env.DefaultImageUrl" class="img-card rounded" style="width: 200px; height: 200px;" />
                                    <div class="grid position-absolute rounded" style="background-image: url(@Model.FindedProfile.AvatarGridURL); ">
                                    </div>
                                </div>
                            }
                            else
                            {
                                <div class="position-relative overflow-hidden p-1 d-flex justify-content-center align-items-center rounded">
                                    <img src="@Model.FindedProfile.UrlAvatar" alt="Avatar" class="img-card rounded" style="width: 200px; height: 200px;" />
                                    <div class="grid position-absolute rounded" style="background-image: url(@Model.FindedProfile.AvatarGridURL); ">
                                    </div>
                                </div>
                            }
                        }
                        else
                        {
                            @if (Model.FindedProfile.UrlAvatar == Targetcom.Env.DefaultImageUrl)
                            {
                                <img src="@Env.DefaultImageUrl" alt="Avatar" class="img-card rounded" style="width: 200px; height: 200px;" />
                            }
                            else
                            {
                                <img src="@Model.FindedProfile.UrlAvatar" alt="Avatar" class="img-card rounded" style="width: 200px; height: 200px;" />
                            }
                        }
                    </div>
                    @if (Model.IdentityProfile.Friendships.FirstOrDefault(f => f.FriendId == Model.FindedProfile.Id) != null)
                    {
                        if (Model.IdentityProfile.Friendships.FirstOrDefault(f => f.FriendId == Model.FindedProfile.Id).FriendStatus == Env.Invite)
                        {
                            <a asp-controller="Friends" asp-action="ViewRemoveFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.FriendId == Model.FindedProfile.Id).Id" class="btn btn-danger text-light pl-4 pr-4" style="font-size: 14px;">Reject query</a>
                        }
                        else if (Model.IdentityProfile.Friendships.FirstOrDefault(f => f.FriendId == Model.FindedProfile.Id).FriendStatus == Env.Friend)
                        {
                            <a asp-controller="Friends" asp-action="ViewRemoveFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.FriendId == Model.FindedProfile.Id).Id" class="btn btn-danger text-light pl-4 pr-4" style="font-size: 14px;">Remove friend</a>
                            <a asp-controller="Friends" asp-action="WriteMessage" asp-route-Id="@Model.FindedProfile.Id" class="btn day_back_main text-light pl-4 pr-4" style="font-size: 14px;">Write message</a>
                        }
                        else if (Model.IdentityProfile.Friendships.FirstOrDefault(f => f.FriendId == Model.FindedProfile.Id).FriendStatus == Env.Subscribe)
                        {
                            <a asp-controller="Friends" asp-action="ViewRemoveFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.FriendId == Model.FindedProfile.Id).Id" class="btn btn-warning text-light pl-4 pr-4" style="font-size: 14px;">Unsubscribe</a>
                        }
                        else if (Model.IdentityProfile.Friendships.FirstOrDefault(f => f.FriendId == Model.FindedProfile.Id).FriendStatus == Env.Blacklist)
                        {
                            <div class="alert alert-danger d-flex justify-content-center align-items-center day_color_main m-0">
                                You in blacklist
                            </div>
                        }
                    }
                    else
                    {
                        if (Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id && f.FriendStatus == Env.Friend) != null)
                        {
                            <a asp-controller="Friends" asp-action="ViewRemoveFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id).Id" class="btn btn-danger text-light pl-4 pr-4" style="font-size: 14px;">Remove friend</a>
                            <a asp-controller="Friends" asp-action="WriteMessage" asp-route-Id="@Model.FindedProfile.Id" class="btn day_back_main text-light pl-4 pr-4" style="font-size: 14px;">Write message</a>
                        }
                        else if (Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id && f.FriendStatus == Env.Invite) != null)
                        {
                            <a asp-controller="Friends" asp-action="ViewAddInviteFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id).Id" class="btn day_back_main text-light pl-4 pr-4" style="font-size: 14px;">Accept friends</a>
                            <a asp-controller="Friends" asp-action="ViewRemoveFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id).Id" class="btn btn-danger text-light pl-4 pr-4" style="font-size: 14px;">Remove query</a>
                            <a asp-controller="Friends" asp-action="ViewLeaveSubscribers" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id).Id" class="btn btn-warning text-light pl-4 pr-4" style="font-size: 14px;">Leave in subscribe</a>
                            <a asp-controller="Friends" asp-action="ViewAddBlacklistFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id).Id" class="btn btn-danger text-light pl-4 pr-4" style="font-size: 14px;">Leave in blacklist</a>
                        }
                        else if (Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id && f.FriendStatus == Env.Subscribe) != null)
                        {
                            <a asp-controller="Friends" asp-action="ViewRemoveFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id).Id" class="btn btn-danger text-light pl-4 pr-4" style="font-size: 14px;">Remove subscriber</a>
                        }
                        else if (Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id && f.FriendStatus == Env.Blacklist) != null)
                        {
                            <a asp-controller="Friends" asp-action="ViewChangeFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id).Id" class="btn day_back_main text-light pl-4 pr-4" style="font-size: 14px;">Add to friend</a>
                            <a asp-controller="Friends" asp-action="ViewLeaveSubscribers" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id).Id" class="btn day_back_main text-light pl-4 pr-4" style="font-size: 14px;">Add to subscribers</a>
                            <a asp-controller="Friends" asp-action="ViewRemoveFriend" asp-route-Id="@Model.IdentityProfile.Friendships.FirstOrDefault(f => f.ProfileId == Model.FindedProfile.Id).Id" class="btn btn-danger text-light pl-4 pr-4" style="font-size: 14px;">Remove with blacklist</a>
                        }
                        else
                        {
                            if (Model.FindedProfile.Privacy == Env.PublicProfile)
                            {
                                <a asp-controller="Friends" asp-action="ViewAddFriend" asp-route-Id="@Model.FindedProfile.Id" class="btn day_back_main text-light pl-4 pr-4" style="font-size: 14px;">Add friends</a>
                            }
                            <a asp-controller="Friends" asp-action="ViewCreateBlacklistFriend" asp-route-Id="@Model.FindedProfile.Id" class="btn btn-danger text-light pl-4 pr-4" style="font-size: 14px;">Leave in blacklist</a>
                        }
                    }
                    @if (Model.FindedProfile.Privacy == Env.PrivateProfile)
                    {
                        <div class="alert alert-danger d-flex justify-content-center align-items-center day_color_main m-0">
                            Private account
                        </div>
                    }
                    else
                    {
                        <div class="alert alert-success d-flex justify-content-center align-items-center day_color_main m-0">
                            Public account
                        </div>
                    }
                </div>
                @if (Model.FindedProfile.Privacy == Env.PublicProfile || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole ||
                    Model.IdentityProfile.Friendships
                        .FirstOrDefault(w => (w.FriendId == Model.IdentityProfile.Id && w.ProfileId == Model.FindedProfile.Id && w.FriendStatus == Env.Friend) ||
                        (w.ProfileId == Model.IdentityProfile.Id && w.FriendId == Model.FindedProfile.Id && w.FriendStatus == Env.Friend)) is not null)
                {
                    if (Model.IdentityProfile.Friendships
                        .FirstOrDefault(w => (w.FriendId == Model.IdentityProfile.Id && w.ProfileId == Model.FindedProfile.Id) ||
                        (w.ProfileId == Model.IdentityProfile.Id && w.FriendId == Model.FindedProfile.Id))
                        is not null)
                    {
                        if (Model.IdentityProfile.Friendships
                        .FirstOrDefault(w => (w.FriendId == Model.IdentityProfile.Id && w.ProfileId == Model.FindedProfile.Id) ||
                        (w.ProfileId == Model.IdentityProfile.Id && w.FriendId == Model.FindedProfile.Id)).FriendStatus != Env.Blacklist)
                        {
                            if (Model.FindedProfile.VisibilityFriends || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                            {
                                if (Model.FindedProfile.Friendships.Where(w => w.FriendStatus == Env.Friend).Count() == 0)
                                {
                                    <div class="border-0 rounded day_back_head day_color_main p-3 overflow-hidden d-flex justify-content-center align-items-center">
                                        Doesnt found friends
                                    </div>
                                }
                                else
                                {
                                    <div class="border-0 rounded day_back_head">
                                        <div class="p-3 overflow-hidden">
                                            <a asp-controller="Friends" asp-action="Index" class="m-0 middletabbtn">Friends: <span class="text-muted">@Model.FindedProfile.Friendships.Where(w => w.FriendStatus == Env.Friend).Count()</span></a>
                                            <hr />
                                            <div class="d-flex flex-wrap" style="gap: .8rem;">
                                                @foreach (var item in Model.FindedProfile.Friendships.Where(w => w.FriendStatus == Env.Friend))
                                                {
                                                    if (item.FriendId == Model.FindedProfile.Id)
                                                    {
                                                        <div class="d-flex flex-column align-items-center p-2 overflow-hidden" style="width: 60px;">
                                                            <img src="@item.Profile.UrlAvatar" class="border-0 rounded-circle" alt="Avatar" style="width: 38px; height: 38px;" />
                                                            <p class="m-0">@item.Profile.Name</p>
                                                        </div>
                                                    }
                                                    else if (item.ProfileId == Model.FindedProfile.Id)
                                                    {
                                                        <div class="d-flex flex-column align-items-center p-2 overflow-hidden" style="width: 60px;">
                                                            <img src="@item.Friend.UrlAvatar" class="border-0 rounded-circle" alt="Avatar" style="width: 38px; height: 38px;" />
                                                            <p class="m-0">@item.Friend.Name</p>
                                                        </div>
                                                    }
                                                }
                                            </div>
                                        </div>
                                    </div>
                                }
                            }
                            if (Model.FindedProfile.VisibilitySubscribers || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                            {
                                if (Model.FindedProfile.Friendships.Where(w => w.FriendId == Model.FindedProfile.Id && w.FriendStatus == Env.Subscribe).Count() == 0)
                                {
                                    <div class="border-0 rounded day_back_head day_color_main p-3 overflow-hidden d-flex justify-content-center align-items-center" style="position: sticky; top: 70px;">
                                        Doesnt found subscribers
                                    </div>
                                }
                                else
                                {
                                    <div class="border-0 rounded day_back_head p-3 overflow-hidden" style="position: sticky; top: 70px;">
                                        <a asp-controller="Friends" asp-action="Subscribers" class="m-0 day_color_main middletabbtn">Subscribers: <span class="day_color_font">@Model.FindedProfile.Friendships.Where(w => w.FriendId == Model.FindedProfile.Id && w.FriendStatus == Env.Subscribe).Count()</span></a>
                                        <hr />
                                        <div class="d-flex flex-wrap" style="gap: .8rem;">
                                            @foreach (var item in Model.FindedProfile.Friendships.Where(w => w.FriendStatus == Env.Subscribe))
                                            {
                                                if (item.FriendId == Model.FindedProfile.Id)
                                                {
                                                    <div class="d-flex flex-column align-items-center p-2 overflow-hidden" style="width: 60px;">
                                                        <img src="@item.Profile.UrlAvatar" class="border-0 rounded-circle" alt="Avatar" style="width: 38px; height: 38px;" />
                                                        <p class="m-0">@item.Profile.Name</p>
                                                    </div>
                                                }
                                            }
                                        </div>
                                    </div>
                                }
                            }
                        }
                    }
                    else
                    {
                        if (Model.FindedProfile.VisibilityFriends || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                        {
                            if (Model.FindedProfile.Friendships.Where(w => w.FriendStatus == Env.Friend).Count() == 0)
                            {
                                <div class="border-0 rounded day_back_head day_color_main p-3 overflow-hidden d-flex justify-content-center align-items-center">
                                    Doesnt found friends
                                </div>
                            }
                            else
                            {
                                <div class="border-0 rounded day_back_head">
                                    <div class="p-3 overflow-hidden">
                                        <a asp-controller="Friends" asp-action="Index" class="m-0 day_color_main middletabbtn">Friends: <span class="day_color_font">@Model.FindedProfile.Friendships.Where(w => w.FriendStatus == Env.Friend).Count()</span></a>
                                        <hr />
                                        <div class="d-flex flex-wrap" style="gap: .8rem;">
                                            @foreach (var item in Model.FindedProfile.Friendships.Where(w => w.FriendStatus == Env.Friend))
                                            {
                                                if (item.FriendId == Model.FindedProfile.Id)
                                                {
                                                    <div class="d-flex flex-column align-items-center p-2 overflow-hidden" style="width: 60px;">
                                                        <img src="@item.Profile.UrlAvatar" class="border-0 rounded-circle" alt="Avatar" style="width: 38px; height: 38px;" />
                                                        <p class="m-0">@item.Profile.Name</p>
                                                    </div>
                                                }
                                                else if (item.ProfileId == Model.FindedProfile.Id)
                                                {
                                                    <div class="d-flex flex-column align-items-center p-2 overflow-hidden" style="width: 60px;">
                                                        <img src="@item.Friend.UrlAvatar" class="border-0 rounded-circle" alt="Avatar" style="width: 38px; height: 38px;" />
                                                        <p class="m-0">@item.Friend.Name</p>
                                                    </div>
                                                }
                                            }
                                        </div>
                                    </div>
                                </div>
                            }
                        }
                        if (Model.FindedProfile.VisibilitySubscribers || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                        {
                            if (Model.FindedProfile.Friendships.Where(w => w.FriendId == Model.FindedProfile.Id && w.FriendStatus == Env.Subscribe).Count() == 0)
                            {
                                <div class="border-0 rounded day_back_head day_color_main p-3 overflow-hidden d-flex justify-content-center align-items-center">
                                    Doesnt found subscribers
                                </div>
                            }
                            else
                            {
                                <div class="border-0 rounded day_back_head p-3 overflow-hidden">
                                    <a asp-controller="Friends" asp-action="Subscribers" class="m-0 day_color_main middletabbtn">Subscribers: <span class="day_color_font">@Model.FindedProfile.Friendships.Where(w => w.FriendId == Model.FindedProfile.Id && w.FriendStatus == Env.Subscribe).Count()</span></a>
                                    <hr />
                                    <div class="d-flex flex-wrap" style="gap: .8rem;">
                                        @foreach (var item in Model.FindedProfile.Friendships.Where(w => w.FriendStatus == Env.Subscribe))
                                        {
                                            if (item.FriendId == Model.FindedProfile.Id)
                                            {
                                                <div class="d-flex flex-column align-items-center p-2 overflow-hidden" style="width: 60px;">
                                                    <img src="@item.Profile.UrlAvatar" class="border-0 rounded-circle" alt="Avatar" style="width: 38px; height: 38px;" />
                                                    <p class="m-0">@item.Profile.Name</p>
                                                </div>
                                            }
                                        }
                                    </div>
                                </div>
                            }
                        }
                    }
                }
            </div>
            <div style="width: 100%" class="gap d-flex flex-column overflow-hidden">

                @if (Model.IdentityProfile.Friendships
          .FirstOrDefault(w => (w.FriendId == Model.IdentityProfile.Id && w.ProfileId == Model.FindedProfile.Id) ||
          (w.ProfileId == Model.IdentityProfile.Id && w.FriendId == Model.FindedProfile.Id))
          is not null)
                {
                    if (Model.IdentityProfile.Friendships
                    .FirstOrDefault(w => (w.FriendId == Model.IdentityProfile.Id && w.ProfileId == Model.FindedProfile.Id) ||
                    (w.ProfileId == Model.IdentityProfile.Id && w.FriendId == Model.FindedProfile.Id)).FriendStatus != Env.Blacklist)
                    {
                        <div class="card day_back_head border-0">
                            <div class="card-body p-3" style="display: grid; font-family: Poppins, sans-serif;">
                                <div class="d-flex flex-column">
                                    <div class="d-flex justify-content-between">
                                        <h4 class="m-0 font-weight-normal d-flex align-items-center" style="font-family: Poppins, sans-serif;">
                                            @if (Model.FindedProfile.RainbowMode == Env.RainbowOn)
                                            {
                                                <span class="rainbow">@Model.FindedProfile.Name @Model.FindedProfile.Surname</span>
                                            }
                                            else
                                            {
                                                <span>@Model.FindedProfile.Name @Model.FindedProfile.Surname</span>
                                            }
                                            @if (Model.FindedProfile.IsVerify)
                                            {<i class="fas fa-check day_color_main ml-2" style="font-size: 13px;"></i>}
                                        </h4>
                                        <p class="m-0 text-muted font-weight-normal" style="font-size: 14px;">@Model.FindedProfile.AfkStatus</p>
                                    </div>
                                    @if (Model.FindedProfile.VisibilityQuote || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                                    {
                                        <p class="m-0" style="font-family: Poppins, sans-serif;">@Model.FindedProfile.Quote</p>
                                    }
                                </div>
                                @if (Model.FindedProfile.Privacy == Env.PublicProfile || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole ||
                                      Model.IdentityProfile.Friendships
                        .FirstOrDefault(w => (w.FriendId == Model.IdentityProfile.Id && w.ProfileId == Model.FindedProfile.Id && w.FriendStatus == Env.Friend) ||
                        (w.ProfileId == Model.IdentityProfile.Id && w.FriendId == Model.FindedProfile.Id && w.FriendStatus == Env.Friend)) is not null)
                                {
                                    <hr style="height: 2px; width: 100%;" />
                                    @if (Model.Role == Env.ModerRole)
                                    {
                                        if (Model.FindedProfile.VisibilityRole || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                                        {
                                            <div class="alert alert-warning p-2 d-flex justify-content-center align-items-center" style="font-family: Poppins, sans-serif;">
                                                <p class="m-0">This user is Moderator <i class="fas fa-glasses" style="font-size: 13px;"></i></p>
                                            </div>
                                        }
                                    }
                                    @if (Model.FindedProfile.IsPremium)
                                    {
                                        <div class="alert alert-warning p-2 d-flex justify-content-center align-items-center" style="font-family: Poppins, sans-serif;">
                                            <p class="m-0">Premium plan (Permanent) <i class="fas fa-crown" style="font-size: 13px;"></i></p>
                                        </div>
                                    }
                                    else
                                    {
                                        <a class="alert middletabbtn1 border-0 p-2 d-flex justify-content-center align-items-center" style="font-family: Poppins, sans-serif;">
                                            <p class="m-0">Doesnt plan</p>
                                        </a>
                                    }
