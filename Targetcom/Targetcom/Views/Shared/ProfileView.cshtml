                                @if (Model.FindedProfile.VisibilityAboutMe || Model.FindedProfile.VisibilityCommerceData || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                                {
                                    <a href="#" class="viewallbtn rounded day_color_main p-2 text-center">View all</a>
                                    <script>
                                        document.querySelector(".viewallbtn").addEventListener("click", () => {
                                            document.querySelector(".viewalldata").classList.toggle("disable");
                                        });
                                    </script>
                                }
                            }
                        </div>
                        <div class="card-footer">
                            <div class="d-flex justify-content-around" style="font-family: Poppins, sans-serif;">
                                @if (Model.FindedProfile.VisibilityFriends || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                                {
                                    <a asp-controller="Friends" asp-action="Index" class="d-flex flex-column align-items-center"><span class="day_color_main" style="font-size: 18px;">@Model.FindedProfile.Friendships.Where(w => w.FriendStatus == Env.Friend).Count()</span> <span class="text-muted">Friends</span></a>
                                }
                                @if (Model.FindedProfile.VisibilitySubscribers || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                                {
                                    <a asp-controller="Friends" asp-action="Subscribers" asp-route-eventSelector="Subscribers" class="d-flex flex-column align-items-center"><span class="day_color_main" style="font-size: 18px;">@Model.FindedProfile.Friendships.Where(w => w.FriendId == Model.FindedProfile.Id && w.FriendStatus == Env.Subscribe).Count()</span> <span class="text-muted">Subscribers</span></a>
                                }
                                @if (Model.FindedProfile.Privacy == Env.PublicProfile)
                                {
                                    @if (Model.FindedProfile.VisibilityImages || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                                    {
                                        <a asp-controller="Friends" asp-action="Images" asp-route-Id="@Model.FindedProfile.Id" class="d-flex flex-column align-items-center"><span class="day_color_main" style="font-size: 18px;">@Model.FindedProfile.ProfilePostages.Where(w => w.IsObject && w.ProfileId == Model.FindedProfile.Id).Count()</span> <span class="text-muted">Images</span></a>
                                    }
                                }
                            </div>
                        </div>
                        @if (Model.FindedProfile.Privacy == Env.PublicProfile || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                        {
                            if (Model.FindedProfile.VisibilityImages || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                            {
                                if (Model.FindedProfile.ImageVidget)
                                {
                                    if (Model.FindedProfile.ProfilePostages.Where(w => w.IsObject).Count() > 0)
                                    {
                                        <div class="border-0 gap d-flex justify-content-around overflow-hidden">
                                            @foreach (var item in Model.FindedProfile.ProfilePostages.Where(w => w.IsObject).OrderByDescending(o => o.TimeStamp).Take(4))
                                            {
                                                <div class="rounded objf d-flex" style="width: 116px; height:116px; background-image: url(@item.Content);">
                                                    <div class="d-flex justify-content-center align-items-center rounded objfeffect flex-column" style="width:100%; height:100%;">
                                                        <i class="far fa-heart text-danger" style="font-size: 35px; transform: translateX(-30%);"></i>
                                                        <h5 class="text-light mt-1">@item.LikedProfiles.Count</h5>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    }
                                }
                            }
                        }
                    </div>
                    @if (Model.FindedProfile.Privacy == Env.PublicProfile || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                    {
                        if (!Model.FindedProfile.IsNessessaredPublishPost || Model.MyRole == Env.AdminRole || Model.MyRole == Env.ModerRole)
                        {
                            @using (Html.BeginForm(FormMethod.Post))
                            {
                                <div class="card border-0 day_back_head overflow-auto rounded">
                                    <div class="card-body p-2">
                                        <textarea class="form-control day_back_head day_color_font item-font m-0 rounded-0 border-0" id="summer" name="content" placeholder="Tell us what's new?"></textarea>
                                    </div>
                                    <div class="card-footer d-flex align-items-center justify-content-between">
                                        <div class="d-flex align-items-center">
                                            @if (Model.IdentityProfile.UrlAvatar == Targetcom.Env.DefaultImageUrl)
                                            {
                                                <img src="@Env.DefaultImageUrl" alt="Avatar" class="img-card rounded-circle" style="width: 38px; height: 38px;" />
                                            }
                                            else
                                            {
                                                <img src="@Model.IdentityProfile.UrlAvatar" alt="Avatar" class="img-card rounded-circle" style="width: 38px; height: 38px; " />
                                            }
                                            <div class="d-flex flex-column ml-2">
                                                <p class="m-0 day_color_main" style="font-size: 14px;">
                                                    @Model.IdentityProfile.Name
                                                    @Model.IdentityProfile.Surname
                                                    @if (Model.IdentityProfile.IsVerify)
                                                    {
                                                        <i class="fas fa-check" style="font-size: 13px;"></i>
                                                    }
                                                </p>
                                            </div>
                                        </div>
                                        <div>
                                            <button type="submit" class="btn day_back_main text-light pl-4 pr-4 disable" style="font-size: 14px;" id="publishpost" asp-controller="Friends" asp-action="PublishPost" asp-route-Id="@Model.FindedProfile.Id">Publish</button>
                                            <script>
                                                document.getElementById("summer").addEventListener("keyup", () => {
                                                    if (document.getElementById("summer").value.length > 0) {
                                                        document.getElementById("publishpost").classList.remove("disable");
                                                    }
                                                    else {
                                                        document.getElementById("publishpost").classList.add("disable");
                                                    }
                                                });
                                            </script>
                                        </div>
                                    </div>
                                </div>
                            }
                        }
                        <div class="d-flex">
                            <div class="tabs" style="width: 100%">


                                @if (Model.IsAllable)
                                {
                                    <input type="radio" id="tab1" name="tab-control" checked>
                                    <input type="radio" id="tab2" name="tab-control">
                                }
                                else
                                {
                                    <input type="radio" id="tab1" name="tab-control">
                                    <input type="radio" id="tab2" name="tab-control" checked>
                                }

                                <ul class="mb-1">
                                    @if (Model.FindedProfile.VisibilityPostage)
                                    {
                                        <li title="All Post">
                                            <a asp-action="ViewProfile" asp-route-id="@Model.FindedProfile.Id" asp-route-listitem="All" class="text-decoration-none day_color_font font-weight-bold">
                                                <i class="far fa-newspaper"></i>
                                                All Post
                                            </a>
                                        </li>
                                    }
                                    @if (Model.FindedProfile.VisibilitySharePostage)
                                    {
                                        <li title="Shared post">
                                            <a asp-action="ViewProfile" asp-route-id="@Model.FindedProfile.Id" asp-route-listitem="sharedpost" class="text-decoration-none day_color_font font-weight-bold">
                                                <i class="fas fa-share-alt"></i>
                                                Shared Post
                                            </a>
                                        </li>
                                    }
                                </ul>

                                @if (Model.FindedProfile.VisibilityPostage && Model.FindedProfile.VisibilitySharePostage)
                                {
                                    <div class="slider"><div class="indicator"></div></div>
                                }
                                else
                                {
                                    <div style="margin-bottom: .6rem;"></div>
                                }
                                <div class="content">
                                    @if (Model.FindedProfile.VisibilityPostage)
                                    {
                                        <section>
                                            @if (Model.FindedProfile.ProfilePostages.OrderByDescending(i => i.TimeStamp).Count() == 0)
                                            {
                                                <div class="d-flex rounded shadow-sm align-content-center justify-content-center day_back_head p-3 pt-5 pb-5">
                                                    <i class="fas fa-globe day_color_main" style="font-size: 30px;"></i>
                                                    <h4 class="m-0 text-muted ml-2">Doenst exists post</h4>
                                                </div>
                                            }
                                            else
                                            {
                                                <div class="d-flex flex-column gap">
                                                    @foreach (var item in Model.FindedProfile.ProfilePostages.OrderByDescending(i => i.TimeStamp))
                                                    {
                                                        var id = Guid.NewGuid();
                                                        <partial name="Components/ProfilePost" model="new Targetcom.Models.ViewModels.ProfilePostComponentVM() { IdentityProfile = Model.IdentityProfile, Postage = item, Guid = id, IsViewedProfile = true, ViewProfileId = Model.FindedProfile.Id }" />
                                                    }
                                                    <div class="p-2 d-flex justify-content-center">
                                                        @if (Model.Current_AllPost_Page > 0)
                                                        {
                                                            <a asp-controller="Friends" asp-action="ViewProfile" asp-route-id="@Model.FindedProfile.Id" asp-route-listitem="@Env.AllPost" asp-route-postpage="@(Model.Current_AllPost_Page - 1)" class="p-3 d-flex align-items-center text-decoration-none text-light day_back_main rounded border-0 navbtn"><i class="fas fa-arrow-left m-0 mr-2"></i>Prev</a>
                                                        }
                                                        @if ((Model.Current_AllPost_Lenght / Env.PROFILE_NEWS_LOADING_LIMIT) > Model.Current_AllPost_Page)
                                                        {
                                                            <a asp-controller="Friends" asp-action="ViewProfile" asp-route-id="@Model.FindedProfile.Id" asp-route-listitem="@Env.AllPost" asp-route-postpage="@(Model.Current_AllPost_Page + 1)" class="p-3 d-flex align-items-center text-decoration-none text-light day_back_main rounded border-0 ml-3 navbtn">Next <i class="fas fa-arrow-right m-0 ml-2"></i></a>
                                                        }
                                                    </div>
                                                </div>
                                            }
                                        </section>
                                    }
                                    @if (Model.FindedProfile.VisibilitySharePostage)
                                    {
                                        <section>
                                            @if (Model.FindedProfile.SharedProfilePostages.Where(w => w.ProfileId == Model.FindedProfile.Id).OrderByDescending(i => i.Postage.TimeStamp).Count() == 0)
                                            {
                                                <div class="d-flex rounded shadow-sm align-content-center justify-content-center day_back_head p-3 pt-5 pb-5">
                                                    <i class="fas fa-share-alt-square day_color_main" style="font-size: 30px;"></i>
                                                    <h4 class="m-0 text-muted ml-2">Doenst exists share post</h4>
                                                </div>
                                            }
                                            else
                                            {
                                                <div class="d-flex flex-column gap">
                                                    @foreach (var item in Model.FindedProfile.SharedProfilePostages.Where(w => w.ProfileId == Model.FindedProfile.Id).OrderByDescending(i => i.Postage.TimeStamp).Select(s => s.Postage))
                                                    {
                                                        var id = Guid.NewGuid();
                                                        <partial name="Components/ProfilePost" model="new Targetcom.Models.ViewModels.ProfilePostComponentVM() { IdentityProfile = Model.IdentityProfile, Postage = item, Guid = id, IsViewedProfile = true, ViewProfileId = Model.FindedProfile.Id }" />
                                                    }
                                                    <div class="p-2 d-flex justify-content-center">
                                                        @if (Model.Current_SharedPost_Page > 0)
                                                        {
                                                            <a asp-controller="Friends" asp-action="ViewProfile" asp-route-id="@Model.FindedProfile.Id" asp-route-listitem="@Env.SharedPost" asp-route-sharepage="@(Model.Current_SharedPost_Page - 1)" class="p-3 d-flex align-items-center text-decoration-none text-light day_back_main rounded border-0 navbtn"><i class="fas fa-arrow-left m-0 mr-2"></i>Prev</a>
                                                        }
                                                        @if ((Model.Current_SharedPost_Lenght / Env.PROFILE_NEWS_LOADING_LIMIT) > Model.Current_SharedPost_Page)
                                                        {
                                                            <a asp-controller="Friends" asp-action="ViewProfile" asp-route-id="@Model.FindedProfile.Id" asp-route-listitem="@Env.SharedPost" asp-route-sharepage="@(Model.Current_SharedPost_Page + 1)" class="p-3 d-flex align-items-center text-decoration-none text-light day_back_main rounded border-0 ml-3 navbtn">Next <i class="fas fa-arrow-right m-0 ml-2"></i></a>
                                                        }
                                                    </div>
                                                </div>
                                            }
                                        </section>
                                    }
                                </div>
                            </div>
                        </div>
                    }
                }
            </div>
        </div>
    }
</div>