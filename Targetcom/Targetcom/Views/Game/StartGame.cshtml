
@{
    ViewData["Title"] = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model Game;
<style>
    .btnback {
        background-color: transparent;
        text-decoration: none;
        color: transparent;
        height: 5vh;
        z-index: 600;
        top: 15px;
        right: 15px;
        font-family: Poppins, sans-serif;
    }
</style>
<iframe src="@Model.GameUrl" class="position-absolute border-0" style="width: 100%; height: 100vh; left: 0; top: 0px; z-index:500; object-fit: none;">
</iframe>
<a asp-action="Index" asp-controller="Game" class="p-2 rounded position-absolute btnback d-flex justify-content-center align-items-center text-decoration-none bg-light">
    <i class="fas fa-times text-danger fa-2x"></i>
</a>